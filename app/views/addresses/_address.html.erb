<div class="address">
  <% present address do |address_presenter| %>
    <strong class="address-type">
      <%= address_presenter.name %>
    </strong>
    <%= address_presenter.line_1 %>
    <%= address_presenter.line_2 %>
    <%= address_presenter.line_3 %>
    <%= address_presenter.city %>
    <%= address_presenter.postcode %>
    <%= address_presenter.edit_link %>
    <%= address_presenter.delete_link %>
  <% end %>
</div>
