<%= simple_form_for @project do |f| %>
  <%= f.input :name %>
  <%= f.input :description, as: :text %>
  <%= f.association :brand %>
  <%= f.association :customer, collection: Customer.order("name ASC") %>
  <%= f.input :purchase_order %>
  <%= f.input :delivery_deadline %>
  <%= f.input :notes, as: :text %>
  <%= f.input :rush_job %>
  <%= f.button :submit %>
<% end %>
