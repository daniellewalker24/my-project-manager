<%= content_tag :div, class: "basic-listing  #{cycle('odd', 'even', name: 'print_job')}", id: dom_id(print_job) do %>
  <div class="print-job-name">
    <%= print_job.name %> - <%= print_job.vat? ? 'VAT' : 'No VAT' %>
  </div>

  <div class="print-job-prices-overview">
    <%= render(partial: 'print_jobs/prices', locals: { print_job: print_job }) %>
  </div>
<% end %>
