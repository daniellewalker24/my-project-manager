var object = '#edit_my_service_item_<%= @my_service_item.id %>';
$(object + ' .flash-message').remove();
<% if @updated %>
  $(object).prepend('<div class="flash-message notice">Success</div>');
<% else %>
  $(object).prepend('<div class="flash-message alert">Failure, please try again</div>');
<% end %>
