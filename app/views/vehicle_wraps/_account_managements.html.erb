<div class="content">
  <div class="main vehicle-wrap-section" id="account-management">
    <h3>Account Management</h3>

    <%= link_to "Add Account Management", new_vehicle_wrap_vehicle_wraps_account_management_path(vehicle_wrap) %>

    <% if vehicle_wrap.account_managements.any? %>
      <%= render(
        partial: "vehicle_wraps/account_managements/account_management",
        collection: vehicle_wrap.account_managements,
        as: :account_management
      ) %>
    <% end %>
  </div>
</div>
